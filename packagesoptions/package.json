{
  "name": "algocontent-packagesOptions-api",
  "version": "1.0.0",
  "description": "Carvana AlgoContent PackagesOptions API",
  "main": "index.js",
  "scripts": {
    "start": "export NODE_ENV=development && nodemon server/index.js --exec babel-node --presets @babel/preset-env",
    "serve": "node build/index.js",
    "build:base": "rm -rf ./build && babel server -d ./build --copy-files",
    "build:dev": "export NODE_ENV=development && npm run build:base",
    "build:test": "export NODE_ENV=test && npm run build:base",
    "build:uat": "export NODE_ENV=uat && npm run build:base",
    "build:prod": "export NODE_ENV=production && npm run build:base",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "test": "./node_modules/.bin/run-p --race serve cy:run",
    "test:ci": "./node_modules/.bin/run-p --race serve cy:run:record"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@azure/cosmos": "^3.5.3",
